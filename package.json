{
  "name": "typescript-storyshots",
  "version": "1.0.0",
  "description": "Sample project using StoryShots, Jest and TypeScript",
  "main": "lib/src/index.js",
  "typings": "lib/src/index.d.ts",
  "scripts": {
    "start": "npm run watch:app & npm run watch:storybook",
    "test": "jest",
    "watch:storybook": "start-storybook -p 6006",
    "watch:app": "webpack-dev-server --config scripts/webpack.config.js --mode development --hot --color --port 3000",
    "watch:data": "nodemon --ext md --exec \"npm run build:static\" ./content/*",
    "build:storybook": "build-storybook",
    "build:static": "node -r ts-node/register -r tsconfig-paths/register src/static/generate.ts",
    "dist": "NODE_ENV=\"production\" npm run build:static && npm run dist:app",
    "dist:app": "webpack -p --config scripts/webpack.config.js --mode \"production\" --progress",
    "profile:app": "webpack --config scripts/webpack.config.js --profile --json > stats.json"
  },
  "dependencies": {
    "@storybook/addon-console": "1.0.0",
    "functional-promise": "1.5.8",
    "grid-styled": "3.2.1",
    "history": "4.7.2",
    "ramda": "0.25.0",
    "react": "16.3.1",
    "react-helmet": "5.2.0",
    "react-loadable": "5.3.1",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-typewriter": "0.4.1",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2",
    "styled-components": "3.2.5",
    "svg-inline-react": "3.0.0",
    "tslib": "1.9.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.0-alpha.2",
    "@storybook/addon-knobs": "4.0.0-alpha.2",
    "@storybook/addon-links": "4.0.0-alpha.2",
    "@storybook/addon-storyshots": "^4.0.0-alpha.2",
    "@storybook/addons": "4.0.0-alpha.2",
    "@storybook/react": "4.0.0-alpha.2",
    "@types/grid-styled": "3.2.0",
    "@types/jest": "^22.2.2",
    "@types/node": "9.6.2",
    "@types/react": "^16.3.11",
    "@types/react-dom": "16.0.5",
    "@types/react-loadable": "5.3.4",
    "@types/react-test-renderer": "^16.0.1",
    "@types/storybook__react": "^3.0.7",
    "@types/webpack-env": "1.13.5",
    "awesome-typescript-loader": "5.0.0-1",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "copy-webpack-plugin": "4.5.1",
    "csstype": "2.1.1",
    "favicons": "5.0.0",
    "fs-extra": "5.0.0",
    "glob": "7.1.2",
    "gray-matter": "4.0.1",
    "html-loader": "0.5.5",
    "html-react-parser": "0.4.3",
    "html-webpack-plugin": "3.2.0",
    "jest": "^22.4.3",
    "jest-environment-jsdom": "22.4.3",
    "jest-styled-components": "5.0.1",
    "prop-types": "15.6.1",
    "react-dom": "16.3.1",
    "react-hot-loader": "4.0.1",
    "react-test-renderer": "^16.3.1",
    "rehype": "5.0.1",
    "rehype-format": "2.2.0",
    "rehype-highlight": "2.1.0",
    "remark-autolink-headings": "5.0.0",
    "remark-frontmatter": "1.2.0",
    "remark-html": "7.0.0",
    "remark-parse": "5.0.0",
    "remark-slug": "5.0.0",
    "require-hacker": "3.0.1",
    "source-map-support": "0.5.4",
    "storybook-react-router": "1.0.1",
    "svg-inline-loader": "0.8.0",
    "ts-jest": "^22.4.2",
    "ts-node": "5.0.1",
    "tsconfig-paths": "3.2.0",
    "tsconfig-paths-webpack-plugin": "3.0.3",
    "typescript": "^2.8.1",
    "uglifyjs-webpack-plugin": "1.2.4",
    "unified": "6.1.6",
    "webpack": "4.5.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cleanup-plugin": "0.5.1",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.1",
    "webpack-log": "1.2.0",
    "webpack-manifest-plugin": "2.0.0-rc.2"
  },
  "jest": {
    "transform": {
      "\\.(jsx?|tsx?)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
